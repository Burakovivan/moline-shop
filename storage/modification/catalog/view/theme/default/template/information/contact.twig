{{ header }}
<div id="information-contact 33">
  <div class="bred_crambs">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<ul class="bred_crambs_list">
						{% for breadcrumb in breadcrumbs %}
							<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a><img src="img/arrow.svg" alt=""></li>
					{% endfor %}
				</ul>
			</div>
		</div>
	</div>
</div>
<section class="contacts">
		<div class="container">
			<div class="col-md-6">
				<div class="contact-group">
						<div class="contact-group-head">{{ text_telephone }}</div>
						{# <div class="contact-group-head">Консльтация и заказ по телефону</div> #}
						<ul class="phones">
							<li><img src="img/contacts_icon/vodafone.svg" alt="">{{ telephone }}</li>
							<li><img src="img/contacts_icon/kvstr.svg" alt="">+38 (097) 100 20 20</li>
							<li><img src="img/contacts_icon/life.svg" alt="">+38 (063) 100 20 20</li>
							<li><i class="fas fa-envelope"></i>sales@moline.com</li>
						</ul>		
				</div>
				<div class="contact-group">
					{# <div class="contact-group-head">Склад (Самовывоз)</div> #}
					<div class="contact-group-head">{{ store }}</div>
					<p><address>{{ address }}</address></p>
					{# <p>ул. Маяковского, 132</p> #}
				</div>
				
				<div class="contact-group">
						<div class="contact-group-head">Выстовочный зал</div>
						<p>ул. Черкесская, 55</p>

						<div class="list_name">График работы:</div>
						<ul class="schedule">
							<li>Пн-Пт: с 9:00 до 12:00</li>
							<li>Сб: с 9:00 до 12:00</li>
							<li>Вс: Выходной</li>
						</ul>
				</div>
			</div>

			<div class="col-md-6">
				<div class="map_wrap">
						<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d672.6197358672272!2d30.419804493181463!3d50.469518946746575!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4cc52eda4959d%3A0x4af5362d99070dd6!2sCherkaska+St%2C+55%2C+Kyiv%2C+02000!5e0!3m2!1sen!2sua!4v1539625182277" width="100%" height="100%" frameborder="0" style="border:0" allowfullscreen></iframe>
				</div>
			</div>
		</div>
	</section>

	
		
  {# <div class="row">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
    <div id="content" class="{{ class }}">{{ content_top }}
      <h1>{{ heading_title }}</h1>
      <h3>{{ text_location }}</h3>
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="row">
            {% if image %}
            <div class="col-sm-3"><img src="{{ image }}" alt="{{ store }}" title="{{ store }}" class="img-thumbnail" /></div>
            {% endif %}
            <div class="col-sm-3"><strong>{{ store }}</strong><br />
              <address>
              {{ address }}
              </address>
              {% if geocode %}
