{{ header }}
<div id="product-category">
  <div class="bred_crambs">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						<ul class="bred_crambs_list">
							{% for breadcrumb in breadcrumbs %}
								<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a><img src="img/arrow.svg" alt=""></li>
							{% endfor %}
						</ul>
					</div>
				</div>
			</div>
		</div>
	<div class="overlay"></div>
	{% if categories %}
		<section class="type_select">
		<div class="container">
			<div class="row">
				<h3>Выберите тип товара:</h3>
			</div>
			<div class="row">

				<ul class="top_cat_list">
					 {% for category in categories %}
					<li>
							<a href="{{ category.href }}">
								<div class="type-item">
									<div class="img_wrap"><img src="image/{{ category.image }}" alt="{{ category.name }}"></div>
									<p>{{ category.name }}</p>
								</div>
							</a>
					</li>
					 {% endfor %}
				</ul>
			</div>
		</div>
	</section>
	{% endif %}

