					</div>
				</div>
			</div>

			<div class="row">
				<!-- <div class="slide_mobile_filters col-md-3 pr-lg-right pn-lg-left prn-md pln-md visible-md visible-lg"> -->
				{{column_left}}

				<!-- <div class="col-md-9 pr-lg-right pn-lg-left prn-md pln-md"> -->
				<div class="block_products grid col-md-9 pr-lg-right pn-lg-left prn-md pln-md">
					<div hidden> {{limit}}</div>
					<div hidden> {{products_count}}</div>
					<div hidden> {{page}}</div>
						{{product_list}}
				</div>
			</div>
			<div class="pagination_wrap text-center">
			{% if least %}
				<button class="secondary_btn category-more-btn" data-page="{{page}}">Еще <span id="least">{{least}}</span> товаров</button>
			{% endif %}
				<div class="c_pagination">
					{{pagination}}
				</div>
			</div>
		</div>
